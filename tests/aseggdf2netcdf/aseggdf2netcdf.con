Control Begin
	LogFile               = aseggdf2netcdf.log
	DummyRun              = false
	ArgusMetaData         = Z:\projects\geophysics_netcdf\metadata\airborne_metadata_tab.txt
	VocabularyFile        = Z:\projects\geophysics_netcdf\metadata\vocabulary.csv

	ASEGGDF2Dir          = Z:\projects\2017_AusAEM_Survey_01\final\temp
	NetCDFOutputDir      = Z:\projects\2017_AusAEM_Survey_01\final\temp

	#ASEGGDF2Dir           = Z:\projects\2017_AusAEM_Survey_01\final\line_data_em
	#NetCDFOutputDir       = Z:\projects\2017_AusAEM_Survey_01\final\line_data_em

	DatafileStart         = 1
	DatafileSubsample     = 1
	OverWriteExistingNcFiles = yes

	AddGeospatialMetadata = no
	AddLineStartEndPoints = no
	AddAlphaShapePolygon  = no
	
	Metadata Begin
		title = Argus.SURVEYNAME
		Conventions = CF-1.6, ACDD-1.3
		featureType = trajectory
		source = Airborne Geophysical Survey
		date_created = 
		metadata_link = http:\\ga.gov.au\\??
		history = Converted from Intrepid Database
		product_version = 1.0
		keywords = airborne, geophysics, gamma-ray, radiometrics
		
		geoscience_australia_airborne_survey_project_number = Argus.PROJECT
		geoscience_australia_source_dataset = 
		client = Argus.OPERATOR
		acquisition_contractor = Argus.CONTRACTOR
		acquisition_vessel_type = Argus.VTYPE
		acquisition_vessel_name = Argus.VESSEL
		acquisition_start_date = Argus.StartDate
		acquisition_end_date = Argus.EndDate
		acquisition_processor = Argus.PROCESSOR
		nominal_minimum_line_spacing = Argus.SPACEMIN
		nominal_maximum_line_spacing = Argus.SPACEMAX
		nominal_height_above_ground = Argus.AGL
		nominal_height_above_sealevel = Argus.ASL
	MetaData End

Control End
